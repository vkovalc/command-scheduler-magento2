        <job name="tkotosz_command_scheduler_process_next_schedule"
             instance="Tkotosz\CommandScheduler\Cron\ProcessNextSchedule" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
    </group>
</config>
